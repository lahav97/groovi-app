{
  "expo": {
    "name": "GrooviApp",
    "slug": "groovi-app",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.groovi.app",
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"],
      "newArchEnabled": false
    },
    "ios": {
      "bundleIdentifier": "com.groovi.app",
      "newArchEnabled": false
    },
    "plugins": [
      "expo-localization",
      "@config-plugins/ffmpeg-kit-react-native"
    ],
    "scheme": "groovi",
    "extra": {
      "eas": {
        "projectId": "46aa4db4-94ed-4428-8aa7-6b1605678369"
      }
    }
  }
}
